{
    "manifest_version": 2,
    "name": "Classic Image Search",
    "version": "1.0",
    "description": "An extremely simple extension to bring classic Google image search to Firefox.",
    "icons": {
        "48": "icons/logo_48.png"
    },
    "browser_action": {
        "default_icon": "icons/logo_48.png"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "menus",
        "activeTab"
    ]
}